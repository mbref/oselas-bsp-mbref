--- a/drivers/mtd/devices/m25p80.c
+++ b/drivers/mtd/devices/m25p80.c
@@ -35,13 +35,6 @@
 #include <linux/spi/spi.h>
 #include <linux/spi/flash.h>
 
-#define HACK_PAGE_SZ_128		/* temp hack to use 128 byte page */
